<tamination title="Cross Flip The Line">

  <tam title="Cross Flip the Line 1/4"
       from="Right-Hand Two-Face Line"
       formation="Two-Faced Line RH">
    <path>
      <move select="Lead Right"/>
      <move select="Forward 2" beats="3"/>
    </path>
    <path>
      <move select="Lead Right" beats="2" scaleY="2"/>
      <move select="Run Right" scaleY=".5" offsetX="2"/>
    </path>
  </tam>

