<tamination title="Flip the Line">

  <tam title="Flip the Line 1/4" from="Right-Hand Wave" formation="Wave RH">
    <path>
      <move select="Lead Right" beats="3.5" scaleY="2" />
      <move select="Forward" />
    </path>
    <path>
      <move select="Run Right" scaleY="1.5" offsetX="1"/>
      <move select="Lead Right" />
    </path>
  </tam>

  <tam title="Flip the Line 1/4" from="Left-Hand Wave" formation="Wave LH">
    <path>
      <move select="Lead Left" beats="3.5" scaleY="2" />
      <move select="Forward" />
    </path>
    <path>
      <move select="Run Left" scaleY="1.5" offsetX="1"/>
      <move select="Lead Left" />
    </path>
  </tam>

  <tam title="Flip the Line 1/4"
       from="Right-Hand Waves"
       formation="Ocean Waves RH BGGB">
    <path>
      <move select="Lead Right" beats="3.5" scaleY="2" />
      <move select="Forward" />
    </path>
    <path>
      <move select="Run Right" scaleY="1.5" />
      <move select="Lead Right" />
    </path>
    <path>
      <move select="Lead Right" />
      <move select="U-Turn Right" offsetX="1"/>
    </path>
    <path>
      <move select="Lead Right" beats="3.5" scaleY="2" />
      <move select="Forward"/>
    </path>
  </tam>

  <tam title="Flip the Line 1/4"
       from="Left-Hand Waves"
       formation="Ocean Waves LH BGGB">
    <path>
      <move select="Lead Left" beats="3.5" scaleY="2" />
      <move select="Forward" />
    </path>
    <path>
      <move select="Lead Left" />
      <move select="U-Turn Left" offsetX="1"/>
    </path>
    <path>
      <move select="Run Left" scaleY="1.5" />
      <move select="Lead Left" />
    </path>
    <path>
      <move select="Lead Left" beats="3.5" scaleY="2" />
      <move select="Forward"/>
    </path>
  </tam>

  <tam title="Flip the Line 1/4"
       from="Right-Hand Tidal Wave"
       formation="Tidal Wave RH BGGB">
    <path>
      <move select="Lead Right" beats="3.5" />
    </path>
    <path>
      <move select="Run Right" scaleY="1.5" offsetX="1" />
      <move select="Lead Right" />
    </path>
    <path>
      <move select="Lead Right" scaleX=".5" scaleY=".5" />
      <move select="U-Turn Right" offsetX=".5" offsetY="-.5"/>
    </path>
    <path>
      <move select="Lead Right" beats="3.5" scaleY="2" />
    </path>
  </tam>

  <tam title="Flip the Line 1/4"
       from="Left-Hand Tidal Wave"
       formation="Tidal Wave LH BGGB">
    <path>
      <move select="Lead Left" beats="3.5" />
    </path>
    <path>
      <move select="Run Left" scaleY="1.5" offsetX="1" />
      <move select="Lead Left" />
    </path>
    <path>
      <move select="Lead Left" scaleX=".5" scaleY=".5" />
      <move select="U-Turn Left" offsetX=".5" offsetY=".5"/>
    </path>
    <path>
      <move select="Lead Left" beats="3.5" scaleY="2" />
    </path>
  </tam>

  <tam title="Flip the Line 1/2" from="Right-Hand Wave" formation="Wave RH">
    <path>
      <move select="Lead Right" beats="3.5" scaleY="2" />
      <move select="Forward 2" offsetX=".5"/>
    </path>
    <path>
      <move select="Run Right" />
      <move select="Lead Right" />
      <move select="Forward" scaleX=".5" />
    </path>
  </tam>

  <tam title="Flip the Line 1/2" from="Left-Hand Wave" formation="Wave LH">
    <path>
      <move select="Lead Left" beats="3.5" scaleY="2" />
      <move select="Forward 2" offsetX=".5"/>
    </path>
    <path>
      <move select="Run Left" />
      <move select="Lead Left" />
      <move select="Forward" scaleX=".5" />
    </path>
  </tam>

  <tam title="Flip the Line 1/2"
       from="Right-Hand Waves"
       formation="Ocean Waves RH BGGB">
    <path>
      <move select="Lead Right" beats="3.5" scaleY="2" />
      <move select="Forward 2" offsetX=".5"/>
    </path>
    <path>
      <move select="Run Right" />
      <move select="Lead Right" />
      <move select="Forward" scaleX=".5" />
    </path>
    <path>
      <move select="Run Right" />
      <move select="Lead Right" />
      <move select="Forward" scaleX=".5" />
    </path>
    <path>
      <move select="Lead Right" beats="3.5" scaleY="2" />
      <move select="Forward 2" offsetX=".5"/>
    </path>
  </tam>

  <tam title="Flip the Line 1/2"
       from="Left-Hand Waves"
       formation="Ocean Waves LH BGGB">
    <path>
      <move select="Lead Left" beats="3.5" scaleY="2" />
      <move select="Forward 2" offsetX=".5"/>
    </path>
    <path>
      <move select="Run Left" />
      <move select="Lead Left" />
      <move select="Forward" scaleX=".5" />
    </path>
    <path>
      <move select="Run Left" />
      <move select="Lead Left" />
      <move select="Forward" scaleX=".5" />
    </path>
    <path>
      <move select="Lead Left" beats="3.5" scaleY="2" />
      <move select="Forward 2" offsetX=".5"/>
    </path>
  </tam>

  <tam title="Flip the Line 1/2"
       from="Right-Hand Tidal Wave"
       formation="Tidal Wave RH BGGB">
    <path>
      <move select="Lead Right" beats="3.5" scaleY="2.5" />
    </path>
    <path>
      <move select="Run Right" scaleX=".5" scaleY=".5" />
      <move select="Lead Right" scaleY=".5" />
    </path>
    <path>
      <move select="Run Right" scaleX=".5" scaleY=".5" />
      <move select="Lead Right" scaleY=".5" />
    </path>
    <path>
      <move select="Lead Right" beats="3.5" scaleY="2.5" />
    </path>
  </tam>

  <tam title="Flip the Line 1/2"
       from="Left-Hand Tidal Wave"
       formation="Tidal Wave LH BGGB">
    <path>
      <move select="Lead Left" beats="3.5" scaleY="2.5" />
    </path>
    <path>
      <move select="Run Left" scaleX=".5" scaleY=".5" />
      <move select="Lead Left" scaleY=".5" />
    </path>
    <path>
      <move select="Run Left" scaleX=".5" scaleY=".5" />
      <move select="Lead Left" scaleY=".5" />
    </path>
    <path>
      <move select="Lead Left" beats="3.5" scaleY="2.5" />
    </path>
  </tam>

  <tam title="Flip the Line 3/4" from="Right-Hand Wave" formation="Wave RH">
    <path>
      <move select="Lead Right" beats="3.5" scaleY="2" />
      <move select="Forward 2" />
      <move select="Extend Right" beats="2" scaleX="2"/>
    </path>
    <path>
      <move select="Run Right" />
      <move select="Lead Right" />
      <move select="Forward 2" />
    </path>
  </tam>

  <tam title="Flip the Line 3/4" from="Left-Hand Wave" formation="Wave LH">
    <path>
      <move select="Lead Left" beats="3.5" scaleY="2" />
      <move select="Forward 2" />
      <move select="Extend Left" beats="2" scaleX="2"/>
    </path>
    <path>
      <move select="Run Left" />
      <move select="Lead Left" />
      <move select="Forward 2" />
    </path>
  </tam>

  <tam title="Flip the Line 3/4"
       from="Right-Hand Waves"
       formation="Ocean Waves RH BGGB">
    <path>
      <move select="Lead Right" beats="3.5" scaleY="2" />
      <move select="Forward 4" />
    </path>
    <path>
      <move select="Run Right" />
      <move select="Lead Right" />
      <move select="Forward 2" />
    </path>
    <path>
      <move select="Run Right" />
      <move select="Lead Right" />
      <move select="Forward 2" />
    </path>
    <path>
      <move select="Lead Right" beats="3.5" scaleY="2" />
      <move select="Forward 2" />
      <move select="Extend Right" beats="2" scaleX="2" scaleY="2"/>
    </path>
  </tam>

  <tam title="Flip the Line 3/4"
       from="Left-Hand Waves"
       formation="Ocean Waves LH BGGB">
    <path>
      <move select="Lead Left" beats="3.5" scaleY="2" />
      <move select="Forward 2" />
      <move select="Extend Left" beats="2" scaleX="2" scaleY="2"/>
    </path>
    <path>
      <move select="Run Left" />
      <move select="Lead Left" />
      <move select="Forward 2" />
    </path>
    <path>
      <move select="Run Left" />
      <move select="Lead Left" />
      <move select="Forward 2" />
    </path>
    <path>
      <move select="Lead Left" beats="3.5" scaleY="2" />
      <move select="Forward 4" />
    </path>
  </tam>

  <tam title="Flip the Line 3/4"
       from="Right-Hand Tidal Wave"
       formation="Tidal Wave RH BGGB">
    <path>
      <move select="Lead Right" beats="3" scaleY="1.5" />
      <move select="Extend Right" beats="2"/>
    </path>
    <path>
      <move select="Run Right" scaleX=".5" />
      <move select="Lead Right" scaleY=".5" />
      <move select="Forward" scaleX="1.5"/>
    </path>
    <path>
      <move select="Run Right" scaleX=".5" scaleY=".5" />
      <move select="Lead Right" scaleY=".5" />
      <move select="Forward" scaleX="1.5"/>
    </path>
    <path>
      <move select="Lead Right" beats="3.5" scaleY="2.5" />
      <move select="Extend Right" beats="2"/>
    </path>
  </tam>

  <tam title="Flip the Line 3/4"
       from="Left-Hand Tidal Wave"
       formation="Tidal Wave LH BGGB">
    <path>
      <move select="Lead Left" beats="3" scaleY="1.5" />
      <move select="Extend Left" beats="2"/>
    </path>
    <path>
      <move select="Run Left" scaleX=".5" />
      <move select="Lead Left" scaleY=".5" />
      <move select="Forward" scaleX="1.5"/>
    </path>
    <path>
      <move select="Run Left" scaleX=".5" scaleY=".5" />
      <move select="Lead Left" scaleY=".5" />
      <move select="Forward" scaleX="1.5"/>
    </path>
    <path>
      <move select="Lead Left" beats="3.5" scaleY="2.5" />
      <move select="Extend Left" beats="2"/>
    </path>
  </tam>

  <tam title="Flip the Line" from="Right-Hand Wave" formation="Wave RH">
    <path>
      <move select="Lead Right" beats="3.5" scaleX=".5" scaleY="2" />
      <move select="Forward 2" />
      <move select="Extend Right" beats="2" scaleX="2" scaleY=".5" />
    </path>
    <path>
      <move select="Run Right" />
      <move select="Lead Right" scaleX=".5" />
      <move select="Forward" />
      <move select="Extend Right" beats="2" scaleX="2" scaleY=".5" />
    </path>
  </tam>

  <tam title="Flip the Line" from="Left-Hand Wave" formation="Wave LH">
    <path>
      <move select="Lead Left" beats="3.5" scaleX=".5" scaleY="2" />
      <move select="Forward 2" />
      <move select="Extend Left" beats="2" scaleX="2" scaleY=".5" />
    </path>
    <path>
      <move select="Run Left" />
      <move select="Lead Left" scaleX=".5" />
      <move select="Forward" />
      <move select="Extend Left" beats="2" scaleX="2" scaleY=".5" />
    </path>
  </tam>

  <tam title="Flip the Line"
       from="Right-Hand Waves"
       formation="Ocean Waves RH BGGB">
    <path>
      <move select="Lead Right" beats="3.5" scaleY="2" />
      <move select="Forward 4" />
    </path>
    <path>
      <move select="Run Right" />
      <move select="Lead Right" />
      <move select="Forward 3" />
    </path>
    <path>
      <move select="Run Right" />
      <move select="Lead Right" scaleX=".5" />
      <move select="Forward" />
      <move select="Extend Right" beats="2" scaleX="2" scaleY="1.5" />
    </path>
    <path>
      <move select="Lead Right" beats="3.5" scaleX=".5" scaleY="2" />
      <move select="Forward 2" />
      <move select="Extend Right" beats="2" scaleX="2" scaleY="1.5" />
    </path>
  </tam>

  <tam title="Flip the Line"
       from="Left-Hand Waves"
       formation="Ocean Waves LH BGGB">
    <path>
      <move select="Lead Left" beats="3.5" scaleX=".5" scaleY="2" />
      <move select="Forward 2" />
      <move select="Extend Left" beats="2" scaleX="2" scaleY="1.5" />
    </path>
    <path>
      <move select="Run Left" />
      <move select="Lead Left" scaleX=".5" />
      <move select="Forward" />
      <move select="Extend Left" beats="2" scaleX="2" scaleY="1.5" />
    </path>
    <path>
      <move select="Run Left" />
      <move select="Lead Left" />
      <move select="Forward 3" />
    </path>
    <path>
      <move select="Lead Left" beats="3.5" scaleY="2" />
      <move select="Forward 4" />
    </path>
  </tam>

  <tam title="Flip the Line"
       from="Right-Hand Tidal Wave"
       formation="Tidal Wave RH BGGB">
    <path>
      <move select="Lead Right" beats="3" scaleY="1.5" />
      <move select="Extend Right" beats="2"/>
    </path>
    <path>
      <move select="Run Right" scaleX=".5" />
      <move select="Lead Right" scaleY=".5" />
      <move select="Extend Right" scaleX="1.5"/>
    </path>
    <path>
      <move select="Run Right" scaleX=".5" scaleY=".5" />
      <move select="Lead Right" scaleY=".5" />
      <move select="Forward 2"/>
      <move select="Extend Right"/>
    </path>
    <path>
      <move select="Lead Right" beats="3.5" scaleY="2.5" />
      <move select="Forward" scaleX="1.5"/>
      <move select="Extend Right" beats="2"/>
    </path>
  </tam>

  <tam title="Flip the Line"
       from="Left-Hand Tidal Wave"
       formation="Tidal Wave LH BGGB">
    <path>
      <move select="Lead Left" beats="3" scaleY="1.5" />
      <move select="Extend Left" beats="2"/>
    </path>
    <path>
      <move select="Run Left" scaleX=".5" />
      <move select="Lead Left" scaleY=".5" />
      <move select="Extend Left" scaleX="1.5"/>
    </path>
    <path>
      <move select="Run Left" scaleX=".5" scaleY=".5" />
      <move select="Lead Left" scaleY=".5" />
      <move select="Forward 2"/>
      <move select="Extend Left"/>
    </path>
    <path>
      <move select="Lead Left" beats="3.5" scaleY="2.5" />
      <move select="Forward" scaleX="1.5"/>
      <move select="Extend Left" beats="2"/>
    </path>
  </tam>


  <tam title="Concentric Flip the Line 1/4" group=" "
       formation="Tidal Wave RH BGGB">
    <path>
      <move select="Lead Right" beats="3.5" scaleX="3" scaleY="2" />
      <move select="Forward" offsetX=".5"/>
    </path>
    <path>
      <move select="Run Right" offsetX="1"/>
      <move select="Lead Right" scaleY=".5"  />
    </path>
    <path>
      <move select="Lead Right" beats="3.5" scaleY="1.5" />
    </path>
    <path>
      <move select="Lead Right"  />
      <move select="U-Turn Right" offsetX=".5" offsetY="-1"/>
    </path>
  </tam>

  <tam title="Concentric Flip the Line 1/2" group=" "
       formation="Tidal Wave RH BGGB">
    <path>
      <move select="Lead Right" beats="3.5" scaleX="3" scaleY="2" />
      <move select="Forward 3"/>
    </path>
    <path>
      <move select="Run Right" offsetX="-1" />
      <move select="Lead Right" scaleX="2" />
      <move select="Forward 2" />
    </path>
    <path>
      <move select="Lead Right" beats="3.5" scaleY="2" />
      <move select="Forward"/>
    </path>
    <path>
      <move select="Run Right" />
      <move select="Lead Right" />
    </path>
  </tam>

  <tam title="Concentric Flip the Line 3/4" group=" "
       formation="Tidal Wave RH BGGB">
    <path>
      <move select="Lead Right" beats="3.5" scaleX="3" scaleY="2" />
      <move select="Forward 2"/>
      <move select="Extend Right" beats="3" scaleX="3.5" scaleY="3"/>
    </path>
    <path>
      <move select="Run Right" offsetX="-1" />
      <move select="Lead Right" scaleX="2" />
      <move select="Forward 3" offsetX=".5" />
    </path>
    <path>
      <move select="Lead Right" beats="3.5" scaleY="2" />
      <move select="Extend Right" beats="2" scaleX="1.5"/>
    </path>
    <path>
      <move select="Run Right" />
      <move select="Lead Right" scaleY="1.5"/>
      <move select="Forward"/>
    </path>
  </tam>

  <tam title="Concentric Flip the Line" group=" "
       formation="Tidal Wave RH BGGB">
    <path>
      <move select="Lead Right" beats="3.5" scaleX="1.5" scaleY="2" />
      <move select="Forward 5" />
      <move select="Extend Right" beats="2" scaleX="2" scaleY="1.5" />
    </path>
    <path>
      <move select="Run Right" offsetX="-1" />
      <move select="Lead Right" scaleX=".5" />
      <move select="Forward 5" offsetX=".5" />
      <move select="Extend Right" beats="2" scaleX="2" scaleY="1.5" />
    </path>
    <path>
      <move select="Lead Right" beats="3.5" scaleX=".5" scaleY="2" />
      <move select="Extend Right" beats="2" scaleX="2" scaleY=".5" />
    </path>
    <path>
      <move select="Run Right" />
      <move select="Lead Right" scaleX=".5" />
      <move select="Forward" />
      <move select="Extend Right" scaleX="1.25" scaleY=".5" />
    </path>
  </tam>

  <tam title="Tandem Flip the Line 1/4" group="  "
       formation="Ocean Waves RH BGGB Compact">
    <path>
      <move select="Lead Right" beats="3" scaleX="2.5" scaleY="2"/>
    </path>
    <path>
      <move select="Run Right" beats="4" offsetX="-.5"/>
      <move select="Quarter Right" beats="2" offsetX="1"/>
    </path>
    <path>
      <move select="Forward 3" beats="2"/>
      <move select="Run Right" scaleX="1.5" scaleY="2" offsetX="-.5"/>
      <move select="Quarter Right" beats="2" offsetX="1"/>
    </path>
    <path>
      <move select="Quarter Right" beats="3" offsetX="-.5" offsetY="-4"/>
    </path>
  </tam>

  <tam title="Tandem Flip the Line 1/2" group="  "
       formation="Ocean Waves RH BGGB Compact">
    <path>
      <move select="Lead Right" beats="3" scaleX="2.5" scaleY="2"/>
      <move select="Forward 2"/>
    </path>
    <path>
      <move select="Run Right" beats="4" offsetX="-.5"/>
      <move select="Lead Right" beats="2" scaleX="2" scaleY="2"/>
    </path>
    <path>
      <move select="Forward 3" beats="2"/>
      <move select="Run Right" scaleX="1.5" scaleY="2" offsetX="-.5"/>
      <move select="Lead Right" beats="2" scaleX="2" scaleY="2" />
    </path>
    <path>
      <move select="Quarter Right" beats="3" offsetX="-.5" offsetY="-4"/>
      <move select="Forward 2"/>
    </path>
  </tam>

</tamination>
